  "scripts": {
    "//": "NODE_PATH=./src â€” sets root dir for imports https://github.com/facebookincubator/create-react-app/pull/476",
    "start": "NODE_PATH=./src node scripts/start.js",
    "build": "NODE_PATH=./src node scripts/build.js ",
    "test": "NODE_PATH=./src node scripts/test.js --env=jsdom",
    "test-coverage": "NODE_PATH=./src node scripts/test.js --env=jsdom --coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "src/graphql/mocks.js",
      "src/graphql/schema.js",
      "src/configureStore.js",
      "src/createApolloClient.js",
      "src/root.js",
      "src/routes.js",
      "src/index.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "\\.(gql|graphql)$": "<rootDir>/node_modules/jest-transform-graphql",
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "coverageDirectory": "<rootDir>/public/coverage"
  }
}
